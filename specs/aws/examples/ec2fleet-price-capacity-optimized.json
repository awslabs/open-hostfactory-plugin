{
  "Type": "instant",
  "LaunchTemplateConfigs": [{
    "LaunchTemplateSpecification": {
      "LaunchTemplateId": "{{ launch_template_id }}",
      "Version": "{{ launch_template_version | default('$Latest') }}"
    },
    "Overrides": [
      {"InstanceType": "m5.large", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "m5.xlarge", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "m5.2xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "m5n.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "m5n.xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "m5a.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "m5a.xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "m6i.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "m6i.xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "m6a.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "m6a.xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "r5.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "r5.xlarge", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "r5n.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "r5a.large", "SubnetId": "{{ subnet_ids[0] }}"},
      {"InstanceType": "r6i.large", "SubnetId": "{{ subnet_ids[1] if subnet_ids|length > 1 else subnet_ids[0] }}"},
      {"InstanceType": "r6a.large", "SubnetId": "{{ subnet_ids[0] }}"}
    ]
  }],
  "TargetCapacitySpecification": {
    "TotalTargetCapacity": "{{ requested_count }}",
    "DefaultTargetCapacityType": "spot",
    "OnDemandTargetCapacity": "{{ on_demand_capacity | default(1) }}",
    "OnDemandAllocationStrategy": "prioritized",
    "SpotAllocationStrategy": "price-capacity-optimized"
  },
  "TagSpecifications": [{
    "ResourceType": "fleet",
    "Tags": [
      {"Key": "Name", "Value": "price-optimized-fleet-{{ request_id }}"},
      {"Key": "RequestId", "Value": "{{ request_id }}"},
      {"Key": "TemplateId", "Value": "{{ template_id }}"},
      {"Key": "CreatedBy", "Value": "{{ package_name }}"},
      {"Key": "AllocationStrategy", "Value": "price-capacity-optimized"}
    ]
  }]
}
