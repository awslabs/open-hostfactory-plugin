# Local environment variables for act (GitHub Actions local execution)
# Copy this file to .env.local and customize as needed

# GitHub token for API access (optional - for private repos or rate limiting)
# GITHUB_TOKEN=your_github_token_here

# AWS credentials for testing (if needed)
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
# AWS_DEFAULT_REGION=us-east-1

# Python version for testing
PYTHON_VERSION=3.11

# Testing environment
ENVIRONMENT=testing
TESTING=true

# Package information
PACKAGE_NAME=open-hostfactory-plugin
VERSION=0.1.0

# Disable interactive prompts
CI=true
