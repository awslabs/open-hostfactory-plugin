{
  "Type": "maintain",
  "LaunchTemplateConfigs": [{
    "LaunchTemplateSpecification": {
      "LaunchTemplateId": "{{ launch_template_id }}",
      "Version": "{{ launch_template_version | default('$Latest') }}"
    },
    "Overrides": [
      {"InstanceType": "r6i.large", "Priority": 1},
      {"InstanceType": "r6a.xlarge", "Priority": 2},
      {"InstanceType": "r5.large", "Priority": 3},
      {"InstanceType": "r5n.xlarge", "Priority": 4},
      {"InstanceType": "r5a.2xlarge", "Priority": 5},
      {"InstanceType": "r5d.large", "Priority": 6},
      {"InstanceType": "r5dn.xlarge", "Priority": 7},
      {"InstanceType": "r6id.large", "Priority": 8},
      {"InstanceType": "m6i.large", "Priority": 9},
      {"InstanceType": "m6a.xlarge", "Priority": 10},
      {"InstanceType": "m5.large", "Priority": 11},
      {"InstanceType": "m5n.xlarge", "Priority": 12},
      {"InstanceType": "m5a.2xlarge", "Priority": 13},
      {"InstanceType": "m5d.large", "Priority": 14},
      {"InstanceType": "m5dn.xlarge", "Priority": 15}
    ]
  }],
  "TargetCapacitySpecification": {
    "TotalTargetCapacity": "{{ requested_count }}",
    "DefaultTargetCapacityType": "spot",
    "SpotAllocationStrategy": "capacity-optimized-prioritized"
  },
  "TagSpecifications": [{
    "ResourceType": "fleet",
    "Tags": [
      {"Key": "Name", "Value": "prioritized-fleet-{{ request_id }}"},
      {"Key": "RequestId", "Value": "{{ request_id }}"},
      {"Key": "TemplateId", "Value": "{{ template_id }}"},
      {"Key": "CreatedBy", "Value": "{{ package_name }}"},
      {"Key": "AllocationStrategy", "Value": "capacity-optimized-prioritized"}
    ]
  }]
}
